<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>DeIdentifiedPatientDataExportModule</id>
	<name>DeIdentifiedPatientDataExportModule Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.DeIdentifiedPatientDataExportModule</package>
	<author>Sara Fatima</author>
	<description>
		A module to export patient data in de-identified manner.
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/DeIdentifiedPatientDataExportModule/update.rdf</updateURL>
	<require_version>1.8.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.DeIdentifiedPatientDataExportModule.DeIdentifiedPatientDataExportModuleActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.DeIdentifiedPatientDataExportModule.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		ExportEntity.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with DeIdentifiedPatientDataExportModule. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

